{
  "type": "module",
  "name": "ams2-server-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --no-source-maps --no-cache --no-optimize",
    "build:secure": "parcel build index.html --no-source-maps --no-cache --public-url ./ --no-optimize",
    "lint": "eslint src/**/*.{js,jsx} --ignore-pattern 'src/__tests__/**' --ignore-pattern 'src/setupTests.js'",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "buffer": "^6.0.3",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "parcel": "^2.15.4",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10"
  }
}
